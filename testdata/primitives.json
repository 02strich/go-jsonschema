{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://example.com/primitives",
  "type": "object",
  "properties": {
    "myString": {
      "type": "string"
    },
    "myNumber": {
      "type": "number"
    },
    "myBoolean": {
      "type": "boolean"
    },
    "myNull": {
      "type": "null"
    },

    "myStringArray": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "myNumberArray": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "myBooleanArray": {
      "type": "array",
      "items": {
        "type": "boolean"
      }
    },
    "myNullArray": {
      "type": "array",
      "items": {
        "type": "null"
      }
    }
  }
}
